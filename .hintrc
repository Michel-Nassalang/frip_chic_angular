{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off",
        "duplicate-id-active": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "disown-opener": "off",
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off",
        "aria-required-children": "off"
      }
    ],
    "no-inline-styles": "off",
    "button-type": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 106",
    "not chrome <= 106",
    "not edge <= 106",
    "not ios_saf <= 16",
    "not opera <= 91",
    "not safari <= 16",
    "not samsung <= 18"
  ]
}